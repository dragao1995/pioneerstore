<?xml version="1.0" encoding="UTF-8"?>
<web-app id="jsf" version="2.4" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

<display-name>先锋软件</display-name>
  <description>
     先锋，让你体验科技！
  </description>
  
  
  
  
  
  
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FileUploadServlet</servlet-name>
    <servlet-class>com.pioneer.app.file.FileUploadServlet</servlet-class>
  </servlet>
  	<servlet>
	    <description>This is the description of my J2EE component</description>
	    <display-name>This is the display name of my J2EE component</display-name>
	    <servlet-name>CDSServlet</servlet-name>
	    <servlet-class>com.pioneer.app.xml.action.CDSServlet</servlet-class>
	    <init-param>
	            <param-name>port</param-name>
	            <param-value>8899</param-value>
	    </init-param>
	    <init-param>
	            <param-name>context</param-name>
	            <param-value>store</param-value>
	    </init-param>
	    <load-on-startup>0</load-on-startup>
	   
	</servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TreeServlet</servlet-name>
    <servlet-class>com.pioneer.app.xml.action.TreeServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GoodCDSServlet</servlet-name>
    <servlet-class>com.pioneer.app.xml.action.GoodCDSServlet</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SystemDefServlet</servlet-name>
    <servlet-class>com.pioneer.app.xml.action.SystemDefServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShaPanServlet</servlet-name>
    <servlet-class>com.pioneer.app.xml.action.ShaPanServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>FileUploadServlet</servlet-name>
    <url-pattern>/FileUploadServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CDSServlet</servlet-name>
    <url-pattern>/CDSServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TreeServlet</servlet-name>
    <url-pattern>/TreeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoodCDSServlet</servlet-name>
    <url-pattern>/GoodCDSServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SystemDefServlet</servlet-name>
    <url-pattern>/SystemDefServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShaPanServlet</servlet-name>
    <url-pattern>/shaPanServlet</url-pattern>
  </servlet-mapping>
  
 
  
 
 
  

    
    <!-- listener>
    	<listener-class>kpi.MessageMg</listener-class>
    </listener-->
    
    

   <welcome-file-list>
        <welcome-file>index.jsp</welcome-file> 
    </welcome-file-list>
    
    
</web-app>
